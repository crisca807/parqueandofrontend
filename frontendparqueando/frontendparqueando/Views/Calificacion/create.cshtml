@model WebApplicationParqueando.Models.DTO.CalificacionDTO

<h1>Crear nueva Calificación</h1>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Valoracion" class="control-label"></label>
        <input asp-for="Valoracion" class="form-control" />
        <span asp-validation-for="Valoracion" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="IDUsuario" class="control-label"></label>
        <input asp-for="IDUsuario" class="form-control" />
        <span asp-validation-for="IDUsuario" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="IDEstablecimiento" class="control-label"></label>
        <input asp-for="IDEstablecimiento" class="form-control" />
        <span asp-validation-for="IDEstablecimiento" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Crear" class="btn btn-primary" />
    </div>
</form>

<div>
    <a asp-action="Index">Volver a la lista</a>
</div>
