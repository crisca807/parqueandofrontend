@model WebApplicationParqueando.Models.DTO.ReservaDTO

<h2>Crear Reserva</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="TiempoReserva"></label>
        <input asp-for="TiempoReserva" class="form-control" />
        <span asp-validation-for="TiempoReserva" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Placa"></label>
        <input asp-for="Placa" class="form-control" />
        <span asp-validation-for="Placa" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="TipoVehiculo"></label>
        <input asp-for="TipoVehiculo" class="form-control" />
        <span asp-validation-for="TipoVehiculo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="IDUsuario"></label>
        <input asp-for="IDUsuario" class="form-control" />
        <span asp-validation-for="IDUsuario" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="IDEstablecimiento"></label>
        <input asp-for="IDEstablecimiento" class="form-control" />
        <span asp-validation-for="IDEstablecimiento" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Guardar" class="btn btn-primary" />
    </div>
</form>

<div>
    <a asp-action="Index">Volver a la lista</a>
</div>
