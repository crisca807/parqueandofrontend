@model WebApplicationParqueando.Models.DTO.ComentarioDTO

<h1>Detalles del Comentario</h1>

<div>
    <h4>Comentario</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">ID</dt>
        <dd class="col-sm-10">@Model.IDcomentario</dd>
        <dt class="col-sm-2">Texto del Comentario</dt>
        <dd class="col-sm-10">@Model.ComentarioTexto</dd>
        <dt class="col-sm-2">ID Usuario</dt>
        <dd class="col-sm-10">@Model.IDusuario</dd>
        <dt class="col-sm-2">ID Establecimiento</dt>
        <dd class="col-sm-10">@Model.IDEstablecimiento</dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Volver a la Lista</a>
</div>
